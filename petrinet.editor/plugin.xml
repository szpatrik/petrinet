<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated petrinet -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="hu.bme.mit.petrinet.presentation.PetrinetModelWizardID"
            name="%_UI_PetrinetModelWizard_label"
            class="hu.bme.mit.petrinet.presentation.PetrinetModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PetrinetModelFile.gif">
         <description>%_UI_PetrinetModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated petrinet -->
      <editor
            id="hu.bme.mit.petrinet.presentation.PetrinetEditorID"
            name="%_UI_PetrinetEditor_label"
            icon="icons/full/obj16/PetrinetModelFile.gif"
            extensions="petrinet"
            class="hu.bme.mit.petrinet.presentation.PetrinetEditor"
            contributorClass="hu.bme.mit.petrinet.presentation.PetrinetActionBarContributor">
      </editor>
   </extension>

</plugin>
